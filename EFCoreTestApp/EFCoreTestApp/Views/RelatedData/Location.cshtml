@model IEnumerable<EFCoreTestApp.Models.ConcatLocation>
@{
    ViewBag.Title = "ConcatLocation";
    Layout = "_Layout";
}

<tabel class="table table-striped table-sm">
    <tr>
        <th>ID</th>
        <th>Name</th>
    </tr>
    @foreach(var s in Model)
    {
        <tr>
            <td>@s.ID</td>
            <td>@s.LocationName</td>
        </tr>
    }
</tabel>